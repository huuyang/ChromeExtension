{
	"manifest_version": 2,
	"name": "我的时钟",
	"version": "1.0",
	"description": "我的第二个Chrome扩展",
	"permissions": [ "notifications", "activeTab" ],
	"web_accessible_resources": [ "live.png" ],
	"icons": {
		"16": "16.png",
		"32": "32.png",
		"48": "48.png",
		"128": "128.png"
	},
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_popup": "popup.html"
	}
}